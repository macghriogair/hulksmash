{
    "name": "macghriogair/hulksmash",
    "description": "PHP Project Scaffolder.",
    "keywords": ["php", "scaffold", "boilerplate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Mac Gregor",
            "email": "macgregor.porta@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel-zero/framework": "4.0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "php-mock/php-mock-phpunit": "^2.0",
        "phpunit/phpunit": "6.4",
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["hulksmash"]
}
